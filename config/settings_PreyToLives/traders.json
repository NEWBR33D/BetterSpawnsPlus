{
    "mod": "PreyToLive-BetterSpawnsPlus-v2.0.0",
    "description": "Traders Settings (PreyToLive's recommended settings for BetterSpawnsPlus)",

    "enabled": true,

    "traderProfileImages": {
        "replace": true
    },

    "traderPurchasesFiR": false,
    "traderPurchasesMultiplier": 1,

    "traderRepairRandomDurabilityLoss": true,
    "traderRepairPriceMultiplier": 0.75,

    "prapor": {
        "stockUpdateInMinutes": 30,
        "insurance": {
            "priceMultiplier": 0.12,
            "returnChancePercent": 85,
            "returnTimeInHours": {
                "min": 0,
                "max": 1
            },
            "storageTimeInHours": 168 
        },
        "repairs": {
            "qualityDegradation": 0.9
        }
    },

    "therapist": {
        "stockUpdateInMinutes": 30,
        "insurance": {
            "priceMultiplier": 0.21,
            "returnChancePercent": 100,
            "returnTimeInHours": {
                "min": 0,
                "max": 2
            },
            "storageTimeInHours": 336
        }
    },

    "fence": {
        "stockUpdateInMinutes": 30,
        "disableBlacklist": true,
        "itemAmount": 50,
        "weaponDurabilityPercent": {
            "min": 60,
            "max": 100
        },
        "armorDurabilityPercent": {
            "min": 60,
            "max": 100
        }
    },

    "skier": {
        "stockUpdateInMinutes": 30,
        "repairs": {
            "qualityDegradation": 0.45
        }
    },

    "peacekeeper": {
        "stockUpdateInMinutes": 30
    },

    "mechanic": {
        "stockUpdateInMinutes": 30,
        "repairs": {
            "qualityDegradation": 0
        }
    },

    "ragman": {
        "stockUpdateInMinutes": 30,
        "services": {
            "unlockAllClothingForBothFactions": true,
            "unlockAllClothingForFree": false
        }
    },

    "jaeger": {
        "stockUpdateInMinutes": 30
    }
}
